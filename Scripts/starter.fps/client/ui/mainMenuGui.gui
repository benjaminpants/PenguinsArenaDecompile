function lancer_le_jeu()
{
	if ($BuildType == 40)
	{
		Canvas.setContent(chooseGameGuiPub);
	}
	else
	{
		Canvas.setContent(chooseGameGui);
	}
}

function quitter_le_jeu()
{
	if ($BuildType == 40)
	{
		Canvas.setContent(EndScreenGUI);
	}
	else
	{
		quit();
	}
}

