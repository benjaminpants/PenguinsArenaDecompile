function MainMenuGui::onWake(%__unused)
{
	boutonsPoisson();
	if ($BuildType == 22)
	{
		RSStop.setVisible(0);
		RSSFeedMLText.setVisible(0);
		inviteafriendGUI.setVisible(0);
		ClosedMenuGUI.setVisible(0);
	}
	if ($BuildType == 25)
	{
		RSStop.setVisible(0);
		RSSFeedMLText.setVisible(0);
		inviteafriendGUI.setVisible(0);
		logoFrog.setVisible(0);
		ClosedMenuGUI.setVisible(0);
	}
	if (!alxIsPlaying($MusiqueMenu))
	{
		alxStopAll();
		$currentMusique = "MusiqueMenu";
		if ($pref::Audio::Music)
		{
			$MusiqueMenu = alxPlay("MusiqueMenu");
		}
	}
	if (!$pref::RSS::disableFeedCheck)
	{
	}
	button_quit.setActive(0);
	button_quit.schedule(700, setActive, 1);
}

function boutonsPoisson()
{
	button_bug.setVisible(0);
	button_buy.setVisible(0);
	button_register.setVisible(0);
	if ($BuildType == 40)
	{
		button_buy.setVisible(1);
		button_register.setVisible(1);
	}
	else if ($BuildType == 20)
	{
		button_bug.setVisible(1);
	}
}

package GameTypeCheckVersion
{
	function checkGameVersion()
	{
		if (!$version_already_checked)
		{
			ButtonChooseGameGui.setActive(0);
			ButtonChooseGameGui.setVisible(0);
			CheckingVersionText.setVisible(1);
			echo("Check Version - Checking...");
			schedule(50, 0, "checkVersion");
			schedule(5000, 0, "stopCheckingVersion");
		}
	}

};
function kickOffRSS()
{
	new TCPObject(RSSFeedObject);
	RSSFeedObject.connect($RSSFeed::serverName @ ":" @ $RSSFeed::serverPort);
}

function getTouche(%cmd)
{
	%temp = moveMap.getBinding(%cmd);
	%mapString = "";
	%count = getFieldCount(%temp);
	for (%i = 0; %i < %count; %i += 2)
	{
		if (%mapString !$= "")
		{
			%mapString = %mapString @ ", ";
		}
		%device = getField(%temp, %i + 0);
		%object = getField(%temp, %i + 1);
		%mapString = %mapString @ getMapDisplayName(%device, %object);
	}
	return %mapString;
}

