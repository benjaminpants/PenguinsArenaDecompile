function PlayGui::init_TeamGUI(%__unused, %myTeam, %nbTeams, %teamCouronne)
{
	counter.setVisible(0);
	msgBulle.setVisible(0);
	msgContainer.setVisible(0);
	DEMOmsgContainer.setVisible(0);
	DEMOmsgContainerFantome.setVisible(0);
	CinemaGUI.setVisible(0);
	fond_invincibilite.setVisible(0);
	bonus_invincibilite.setVisible(0);
	bonus_griffes.setVisible(0);
	fond_cactouche.setVisible(0);
	Scores1GUI.setVisible(0);
	Scores2GUI.setVisible(0);
	crosshair.setVisible(1);
	div_team2.setVisible(1);
	div_team3.setVisible(1);
	div_team4.setVisible(1);
	bluen.setVisible(1);
	redn.setVisible(1);
	yellown.setVisible(1);
	greenn.setVisible(1);
	if (%teamCouronne == 1)
	{
		%couronne_bleu = "C";
	}
	else if (%teamCouronne == 2)
	{
		%couronne_rouge = "C";
	}
	else if (%teamCouronne == 3)
	{
		%couronne_jaune = "C";
	}
	else if (%teamCouronne == 4)
	{
		%couronne_vert = "C";
	}
	%div_myteam_x = getWord(div_myteam.getPosition(), 0);
	%div_myteam_y = getWord(div_myteam.getPosition(), 1);
	if (%myTeam == 1)
	{
		div_myteam.setBitmap("starter.fps/client/ui/pingouinBleu" @ %couronne_bleu @ ".png");
		Penguin_Image_GUI.setBitmap("starter.fps/client/ui/objects/penguinBleu.png");
		bluen.setPosition(%div_myteam_x + 98, %div_myteam_y + 100);
		bluename.setPosition(%div_myteam_x + 10, %div_myteam_y + 140);
		bluename.setExtent(130, 33);
	}
	else if (%myTeam == 2)
	{
		div_myteam.setBitmap("starter.fps/client/ui/pingouinRouge" @ %couronne_rouge @ ".png");
		Penguin_Image_GUI.setBitmap("starter.fps/client/ui/objects/penguinRouge.png");
		redn.setPosition(%div_myteam_x + 98, %div_myteam_y + 100);
		redname.setPosition(%div_myteam_x + 10, %div_myteam_y + 140);
		redname.setExtent(130, 33);
	}
	else if (%myTeam == 3)
	{
		div_myteam.setBitmap("starter.fps/client/ui/pingouinJaune" @ %couronne_jaune @ ".png");
		Penguin_Image_GUI.setBitmap("starter.fps/client/ui/objects/penguinJaune.png");
		yellown.setPosition(%div_myteam_x + 98, %div_myteam_y + 100);
		yellowname.setPosition(%div_myteam_x + 10, %div_myteam_y + 140);
		yellowname.setExtent(130, 33);
	}
	else if (%myTeam == 4)
	{
		div_myteam.setBitmap("starter.fps/client/ui/pingouinVert" @ %couronne_vert @ ".png");
		Penguin_Image_GUI.setBitmap("starter.fps/client/ui/objects/penguinVert.png");
		greenn.setPosition(%div_myteam_x + 98, %div_myteam_y + 100);
		greenname.setPosition(%div_myteam_x + 10, %div_myteam_y + 140);
		greenname.setExtent(130, 33);
	}
	%current_div_i = 2;
	for (%team_num = 1; %team_num <= %nbTeams; %team_num++)
	{
		if (%team_num != %myTeam)
		{
			%current_div = "div_team" @ %current_div_i;
			%current_div_position = eval(%current_div @ ".getPosition();");
			%current_div_x = getWord(%current_div_position, 0);
			%current_div_y = getWord(%current_div_position, 1);
			if (%team_num == 1)
			{
				eval(%current_div @ ".setBitmap(\"starter.fps/client/ui/pingouinBleu" @ %couronne_bleu @ ".png\");");
				bluen.setPosition(%current_div_x + 64, %current_div_y + 59);
				bluename.setPosition(%current_div_x + 0, %current_div_y + 90);
				bluename.setExtent(100, 33);
			}
			else if (%team_num == 2)
			{
				eval(%current_div @ ".setBitmap(\"starter.fps/client/ui/pingouinRouge" @ %couronne_rouge @ ".png\");");
				redn.setPosition(%current_div_x + 64, %current_div_y + 59);
				redname.setPosition(%current_div_x + 0, %current_div_y + 90);
				redname.setExtent(100, 33);
			}
			else if (%team_num == 3)
			{
				eval(%current_div @ ".setBitmap(\"starter.fps/client/ui/pingouinJaune" @ %couronne_jaune @ ".png\");");
				yellown.setPosition(%current_div_x + 64, %current_div_y + 59);
				yellowname.setPosition(%current_div_x + 0, %current_div_y + 90);
				yellowname.setExtent(100, 33);
			}
			else if (%team_num == 4)
			{
				eval(%current_div @ ".setBitmap(\"starter.fps/client/ui/pingouinVert" @ %couronne_vert @ ".png\");");
				greenn.setPosition(%current_div_x + 64, %current_div_y + 59);
				greenname.setPosition(%current_div_x + 0, %current_div_y + 90);
				greenname.setExtent(100, 33);
			}
			%current_div_i = %current_div_i + 1;
		}
	}
	for (%team_num = %nbTeams + 1; %team_num <= 4; %team_num++)
	{
		%current_div = "div_team" @ %team_num;
		eval(%current_div @ ".setVisible(0);");
		if (%team_num == 3)
		{
			yellown.setVisible(0);
			yellowscore.setVisible(0);
			etoile_team3.setVisible(0);
		}
		else if (%team_num == 4)
		{
			greenn.setVisible(0);
			greenscore.setVisible(0);
			etoile_team4.setVisible(0);
		}
	}
}

function PlayGui::update_TeamGUI(%__unused, %myTeam, %nbTeams, %current_team, %controled_by_player, %dead, %player_name)
{
	if (%myTeam == %current_team && %dead)
	{
		div_myteam.setBitmap("starter.fps/client/ui/pingouinMort.png");
	}
	if (%myTeam == %current_team)
	{
		echo("----------------" SPC %player_name);
		if (%myTeam == 1)
		{
			bluename.setText(%player_name);
		}
		else if (%myTeam == 2)
		{
			redname.setText(%player_name);
		}
		else if (%myTeam == 3)
		{
			yellowname.setText(%player_name);
		}
		else if (%myTeam == 4)
		{
			greenname.setText(%player_name);
		}
	}
	%current_i = 2;
	for (%team_num = 1; %team_num <= %nbTeams; %team_num++)
	{
		%current_etoile = "etoile_team" @ %current_i;
		%current_div = "div_team" @ %current_i;
		if (%team_num != %myTeam)
		{
			if (%team_num == %current_team)
			{
				if (%controled_by_player)
				{
					eval(%current_etoile @ ".setVisible(1);");
					if (%team_num == 1)
					{
						bluename.setText(%player_name);
					}
					else if (%team_num == 2)
					{
						redname.setText(%player_name);
					}
					else if (%team_num == 3)
					{
						yellowname.setText(%player_name);
					}
					else if (%team_num == 4)
					{
						greenname.setText(%player_name);
					}
				}
				else
				{
					eval(%current_etoile @ ".setVisible(0);");
					if (%team_num == 1)
					{
						bluename.setText("");
					}
					else if (%team_num == 2)
					{
						redname.setText("");
					}
					else if (%team_num == 3)
					{
						yellowname.setText("");
					}
					else if (%team_num == 4)
					{
						greenname.setText("");
					}
				}
				if (%dead)
				{
					eval(%current_div @ ".setBitmap(\"starter.fps/client/ui/pingouinMort.png\");");
				}
			}
			%current_i = %current_i + 1;
		}
	}
}

function PlayGui::updateScore(%__unused, %team, %score)
{
	if (%team == 1)
	{
		bluescore.setText(%score);
	}
	else if (%team == 2)
	{
		redscore.setText(%score);
	}
	else if (%team == 3)
	{
		yellowscore.setText(%score);
	}
	else if (%team == 4)
	{
		greenscore.setText(%score);
	}
}

