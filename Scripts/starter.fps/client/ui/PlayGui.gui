	}
	else if (%team == 3)
	{
		yellowscoreLTG.setText(%score);
	}
	else if (%team == 4)
	{
		greenscoreLTG.setText(%score);
	}
}

function PlayGui::updateTeamMembers(%__unused, %team, %members, %son)
{
	if (%son)
	{
		alxPlay("son_PenguinDeath");
	}
	if (%members == 0)
	{
		%members = "";
	}
	if (%team == 1)
	{
		bluen.setText(%members);
	}
	else if (%team == 2)
	{
		redn.setText(%members);
	}
	else if (%team == 3)
	{
		yellown.setText(%members);
	}
	else if (%team == 4)
	{
		greenn.setText(%members);
	}
}

